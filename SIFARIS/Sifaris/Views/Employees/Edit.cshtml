@model Sifaris.Models.EmployeesModel

@{
    ViewBag.Title = "कर्मचारी व्यवस्थापन - सम्पादन गर्ने";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="well well-small">

    @using (Html.BeginForm())
    {
        @Html.HiddenFor(model => model.EmployeeId)
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
        <div class="btn pull-right"> @Html.ActionLink("Change Password", "Register", "Account")</div>
        Html.RenderPartial("VUC_Add");
        <br />

        <input type="submit" value="सम्पादन गर्नुहोस" class="btn" />
        <div class="btn">  @Html.ActionLink("पछाडी फर्कनुहोस्", "Index")</div>

    }
</div>

