<fieldset>
    <legend>विवाह दर्ता</legend>
    @Html.HiddenFor(model => model.PanjiId)
    @Html.HiddenFor(model => model.NewMarriageId)
    @Html.HiddenFor(model => model.NewMarriageGroomDetailsId)
    @Html.HiddenFor(model => model.NewMarriageBrideDetailsId)
    @Html.HiddenFor(model => model.NewMarriageInformantId)

    <div class="well well-small">
        <h6>(स्थानीय पन्जिकाधिकारीले भर्ने)</h6>
        <table style="width:100%">
            <tr style="text-align:left;">
                <td>
                    <table>
                        <tr>
                            <th>@Html.LabelFor(model => model.PanjiState)</th>
                            <td>@Html.DropDownListFor(model => model.PanjiState, @SifarishView.Utility.GetAllState(), "छान्नुहोस्")</td>
                        </tr>
                        <tr>
                            <th>@Html.LabelFor(model => model.PanjiDistrict)</th>
                            <td>@Html.DropDownListFor(model => model.PanjiDistrict, @SifarishView.Utility.GetAllDistrict(), "छान्नुहोस्")</td>
                        </tr>
                        <tr>
                            <th>@Html.LabelFor(model => model.PanjiPalika)</th>
                            <td>@Html.DropDownListFor(model => model.PanjiPalika, @SifarishView.Utility.GetAllPalika(), "छान्नुहोस्")</td>
                        </tr>
                        <tr>
                            <th>@Html.LabelFor(model => model.PanjiWardNo)</th>
                            <td>@Html.TextBoxFor(model => model.PanjiWardNo, new { @class = "input-mini", @type = "number" })</td>
                        </tr>
                    </table>
                </td>
                <td>
                    <table class="pull-right">
                        <tr>
                            <th>स्थानीय पन्जिकाधिकारीको नाम</th>
                            <td>@Html.TextBoxFor(model => model.PanjiName, new { @class = "langtranslate" })</td>
                        </tr>
                        <tr>
                            <th>स्थानीय पन्जिकाधिकारीको नाम(In English)</th>
                            <td>@Html.TextBoxFor(model => model.PanjiName_Eng, new { @class = "form-control nep" })</td>
                        </tr>
                        <tr>
                            <th>@Html.LabelFor(model => model.Panji_INVId)</th>
                            <td>@Html.TextBoxFor(model => model.Panji_INVId, new { @class = "form-control nep" })</td>
                        </tr>
                        <tr>
                            <th>@Html.LabelFor(model => model.VivahFaramDartaNo)</th>
                            <td>@Html.TextBoxFor(model => model.VivahFaramDartaNo, new { @class = "form-control nep", @readonly = "readonly" })</td>
                        </tr>
                        <tr>
                            <th>@Html.LabelFor(model => model.FaramDartaMiti)</th>
                            <td>@Html.TextBoxFor(model => model.FaramDartaMiti, new { @class = "nepali-calendar", @placeholder = "मिति हाल्नुहोस" })</td>
                        </tr>
                        <tr>
                            <th>परिवार लगत फाराम नं</th>
                            <td>@Html.TextBoxFor(model => model.PariwarLagatFaramNo, new { @class = "form-control nep" })</td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </div>
    <div class="container">
        <div>
            <p> (सूचकले भर्ने )</p>
            <p>श्री स्थानीय पन्जिकाधिकारीज्यू,</p>
            <p>स्थानीय पन्जिकाधिकारीको कार्यालय,</p>
            <p>वडा नं......,.................... महा/उप/नगरपालिका वा गाउँपालिका,</p>
            <p>..................जिल्ला,................ प्रदेश</p>
            <p>महोदय,</p>
            <p>निम्न लिखित विवरण खुलाई नवजात शिशु जन्मको सूचना दिन आएको छु| कानून अनुसार जन्म दर्ता गरि पाउँ |</p>
        </div>
        <h5>१.विवाहको विवरण</h5>
        <div class="col-sm-4">
            <span><label>विवाहको किसिम</label></span>
            @Html.DropDownListFor(model => model.VivahKisim, @SifarishView.Utility.GetVivahKisim(), "छान्नुहोस्")
            @Html.ValidationMessageFor(model => model.VivahKisim)
        </div>
        <div class="col-sm-4">
            <span><label>विवाह भएको मिति</label></span>
            @Html.TextBoxFor(model => model.VivahBhayekoMiti, new { @class = "nepali-calendar", @placeholder = "मिति हाल्नुहोस" })
            @Html.ValidationMessageFor(model => model.VivahBhayekoMiti)
        </div>
        <h5>विवाह सम्पन्न भएको स्थान</h5>
        <div class="row">
            <div class="span11">
                <div class="span3">
                    @Html.LabelFor(model => model.VivahPradesh)
                </div>
                <div class="span7">
                    @Html.DropDownListFor(model => model.VivahPradesh, @SifarishView.Utility.GetAllState(), "छान्नुहोस्")
                    @Html.ValidationMessageFor(model => model.VivahPradesh)
                </div>
            </div>
            <div class="span11">
                <div class="span3">
                    @Html.LabelFor(model => model.VivahJilla)
                </div>
                <div class="span7">
                    @Html.DropDownListFor(model => model.VivahJilla, @SifarishView.Utility.GetAllDistrict(), "छान्नुहोस्")
                    @Html.ValidationMessageFor(model => model.VivahJilla)
                </div>
            </div>
        </div>
        <div class="row">
            <div class="span11">
                <div class="span3">
                    @Html.LabelFor(model => model.VivahPalika)
                </div>
                <div class="span7">
                    @Html.DropDownListFor(model => model.VivahPalika, @SifarishView.Utility.GetAllPalika(), "छान्नुहोस्")
                    @Html.ValidationMessageFor(model => model.VivahPalika)
                </div>
            </div>
            <div class="span11">
                <div class="span3">
                    @Html.LabelFor(model => model.VivahWardNo)
                </div>
                <div class="span7">
                    @Html.TextBoxFor(model => model.VivahWardNo, new { @class = "input-mini", @type = "number" })
                    @Html.ValidationMessageFor(model => model.VivahWardNo)
                </div>
            </div>
        </div>
        <div class="row">
            <div class="span11">
                <div class="span3">
                    <span><label>गाउँ/टोल</label></span>
                </div>
                <div class="span7">
                    @Html.TextBoxFor(model => model.VivahGaunTole)
                    @Html.ValidationMessageFor(model => model.VivahGaunTole)
                </div>
            </div>
            <div class="span11">
                <div class="span3">
                    <span><label>घर नं.</label></span>
                </div>
                <div class="span7">
                    @Html.TextBoxFor(model => model.VivahGharNo)
                    @Html.ValidationMessageFor(model => model.VivahGharNo)
                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <span><label>विदेशमा विवाह भएमा ठेगाना</label></span>
            @Html.TextBoxFor(model => model.VivahBidesh)
            @Html.ValidationMessageFor(model => model.VivahBidesh)
        </div>
        <div class="col-sm-4">
            <span><label>विदेशमा विवाह भएमा ठेगाना(In English)</label></span>
            @Html.TextBoxFor(model => model.VivahForeign)
            @Html.ValidationMessageFor(model => model.VivahForeign)
        </div>
    </div>
